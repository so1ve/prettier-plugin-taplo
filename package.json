{
  "name": "prettier-plugin-taplo",
  "version": "0.0.0",
  "packageManager": "pnpm@8.6.9",
  "author": "Ray <i@mk1.io> (@so1ve)",
  "description": "Use taplo to format your TOML files with prettier.",
  "keywords": [
    "toml",
    "taplo",
    "prettier",
    "format",
    "formatter"
  ],
  "homepage": "https://github.com/so1ve/prettier-plugin-taplo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/so1ve/prettier-plugin-taplo.git"
  },
  "bugs": {
    "url": "https://github.com/so1ve/prettier-plugin-taplo/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./worker": {
      "require": "./dist/worker.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pkgroll",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "watch": "pkgroll --watch"
  },
  "dependencies": {
    "@taplo/lib": "0.4.0-alpha.2",
    "synckit": "^0.8.5"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.3",
    "@so1ve/eslint-config": "^0.106.0",
    "@types/node": "^18.16.6",
    "@types/prettier": "^2.7.3",
    "bumpp": "^9.1.0",
    "eslint": "^8.40.0",
    "pkgroll": "^1.10.0",
    "prettier": "^2.8.8",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.32.0"
  },
  "peerDependencies": {
    "prettier": "^2.8.8"
  }
}
